{
  "format": "markdown",
  "excludeFiles": [
    "node_modules",
    ".editorconfig",
    ".gitignore",
    ".yaspellerrc",
    "package.json",
    "package-lock.json"
  ],
  "fileExtensions": [
    ".md"
  ],
  "dictionary": [
    "(C|c)ompose",
    "(D|d)ocker",
    "exec",
    "ps",
    "up"
  ],
  "checkYo": true,
  "findRepeatWords": true,
  "flagLatin": true,
  "ignoreDigits": true,
  "ignoreRomanNumerals": true,
  "ignoreUppercase": true,
  "ignoreUrls": true
}
